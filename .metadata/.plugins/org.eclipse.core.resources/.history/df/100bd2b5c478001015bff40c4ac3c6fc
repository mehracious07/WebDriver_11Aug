package DatePicker;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class DatePick {

	public static void main(String[] args) throws InterruptedException {
		// TODO Auto-generated method stub
		WebDriver driver = new ChromeDriver();
		driver.get("https://ui.shadcn.com/docs/components/date-picker");
		Thread.sleep(2000);
		driver.findElement(By.xpath("//body/div[@class='bg-background relative z-10 flex min-h-svh flex-col']/main[@class='flex flex-1 flex-col']/div[@class='container-wrapper flex flex-1 flex-col px-2']/div[1]")).click();

	}

}
